 }).then(res => {\n        ruleForm.doctorId = res.data.id;\n      }).catch(error => {\n        useToast().error(error);\n      });\n    };\n    const resetForm = formEl => {\n      if (!formEl) return;\n      formEl.resetFields();\n    };\n    onMounted(() => {\n      useDoctors().getAllDoctors();\n      if (router.currentRoute.value.params.doctorId) {\n        doctorId.value = router.currentRoute.value.params.doctorId;\n        ruleForm.doctorId = router.currentRoute.value.params.doctorId;\n        getOneDoctor(doctorId.value);\n      }\n    });\n    const __returned__ = {\n      ruleFormRef,\n      storeDoctor,\n      doctors,\n      doctorId,\n      ruleForm,\n      rules,\n      submitForm,\n      getOneDoctor,\n      resetForm,\n      onMounted,\n      reactive,\n      ref,\n      get useDoctors() {\n        return useDoctors;\n      },\n      get usePatients() {\n        return usePatients;\n      },\n      get useToast() {\n        return useToast;\n      },\n      get router() {\n        return router;\n      },\n      TheComponentHeader,\n      get Request() {\n        return Request;\n      },\n      get storeToRefs() {\n        return storeToRefs;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","reactive","ref","useDoctors","usePatients","useToast","router","TheComponentHeader","Request","storeToRefs","ruleFormRef","storeDoctor","doctors","doctorId","ruleForm","name","dateOfBirth","surname","serviceCost","advertisementName","region","homePhone","mobilePhone","city","address","gender","rules","doctorName","required","message","trigger","workTime","type","submitForm","formEl","validate","valid","createPatient","go","error","getOneDoctor","id","url","method","then","res","data","catch","resetForm","resetFields","getAllDoctors","currentRoute","value","params"],"sources":["/Users/air/Documents/crm-fro