@click=\"router.push(menu.path)\"\n            >\n            <el-icon>\n                <Icons :icon_name=\"menu.icon\"/>\n            </el-icon>\n            <router-link :to=\"menu.path\">\n                <span>{{ menu.name }}</span>\n            </router-link>\n            <div v-if=\"menu.notification\" class=\"notification\">\n                <span>{{ menu.notification > 99 ? 99 + '+' : menu.notification }}</span>\n            </div>\n        </el-menu-item>\n    </div>\n</el-menu>\n<div class=\"demo-drawer__footer\">\n    <el-button @click=\"cancelForm\">Chiqish</el-button>\n</div>\n</div>\n</el-drawer>\n</template>\n\n<script setup>\n// import {useUsers} from \"../stores/users.pinia.js\";\nimport { useShops } from \"../stores/shops.pinia.js\";\nimport Sidebar from \"./Sidebar.vue\";\nimport Header from \"./headers/Header.vue\";\n// import {storeToRefs} from \"pinia\";\nimport TopTab from \"@/components/headers/TopTab.vue\";\nimport { onMounted } from \"vue\";\nimport Icons from \"./Icons.vue\";\nimport { ref } from \"vue\";\nimport {useRoute, useRouter} from \"vue-router\";\n\nconst route = useRoute();\nconst router = useRouter();\n// const storeUser = useUsers();\nconst storeShop = useShops();\nconst burgerActive = ref(false);\n// const {isUser, loading} = storeToRefs(storeUser);\n// storeUser.getUserMe();\n\n\nconst menus = [\n{\n    index: \"1\",\n    name: \"Bosh sahifa\",\n    icon: \"home\",\n    notification: \"\",\n    path: \"/\",\n    roles: []\n}\n];\n\nonMounted(() => {\n    storeShop.getShopSubscription()\n})\n</script>\n\n\n\n<style>\n</style>"],"mappings":"AAqFA,SAASA,QAAQ,QAAQ,0BAA0B;AACnD,OAAOC,OAAO,MAAM,eAAe;AACnC,OAAOC,MAAM,MAAM,sBAAsB;AACzC;AACA,OAAOC,MAAM,MAAM,iCAAiC;AACpD,SAASC,SAAS,QAAQ,KAAK;AAC/B,OAAOC,KAAK,MAAM,aAAa;AAC/B,SAASC,GAAG,QAAQ,KAAK;AACzB,SAAQC,QAAQ,EAAEC,SAAS,QAAO,YAAY;;;;;;;;IAT9C;IAWA,MAAMC,KAAK,GAAGF,QAAQ,CAAC,CAAC;IACxB,MAAMG,MAAM,GAAGF,SAAS,CAAC,CAAC;IAC1B;IACA,MAAMG,SAAS,GAAGX,QAAQ,CAAC,CAAC;IAC5B,MAAMY,YAAY,GAAGN,G